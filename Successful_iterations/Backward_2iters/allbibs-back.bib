 number = {},
 pages = {671-674},
 title = {A DC Voltage Dependant Switchable Thin Film Bulk Wave Acoustic Resonator Using Ferroelectric Thin Film},
 volume = {},
 year = {2007}
}

@inproceedings{4264291,
 abstract = {A full elliptic bandstop filter using direct circuit transformations is designed, without optimization, for an X-band application. The proposed structure eliminates a possible spurious resonance in the passband, usually caused by tuning elements or reactive terminations previously required for less than ㎻/4-wavelength resonators.},
 author = {Shin, Sanghoon},
