@article{Misme_1966,
 abstract = {Grâce à un appareillage permettant de faire varier la fréquence de façon continue, on étudie la propagation des ondes millimétriques
entre 50et 67GHz et entre 90et 98GHz environ. Dans la bande des 5mm on a mesuré l’absorption par l’oxygène de l’air et mis en évidence qu’au voisinage du maximum d’absorption vers 60GHz on ne constatait aucune irrégularité provenant des raies visibles à basse pression. De plus on a montré l’existence d’une
irrégularité vers 57GHz correspondant à deux raies voisines. On a mesuré l’absorption par le brouillard et la neige. Dans la bande des 3mm on a mesuré l’absorption par la pluie et montré que cette absorption n’est pas une fonction linéaire du taux de précipitation
lorsqu’il est faible. On a également obtenu quelques mesures de l’absorption par le brouillard et la neige.},
 author = {Pierre Misme},
 doi = {10.1007/bf02997476},
 journal = {Annales des T{\'{e}}l{\'{e}}communications},
 month = {nov},
 number = {11-12},
 pages = {226--234},
 publisher = {Springer Science and Business Media {LLC}},
 title = {{\'{E}}tude exp{\'{e}}rimentale de la propagation des ondes millim{\'{e}}triques dans les bandes de 5 et 3 mm.},
 url = {https://doi.org/10.1007%2Fbf02997476},
 volume = {21},
 year = {1966}
}

@inproceedings{Musonda_2015,
 abstract = {A new synthesis method for the generation of the generalized Chebyshev characteristic polynomials has been presented. The general characteristic function is generated by linear combination of elementary Chebyshev characteristic functions. The characteristic function is suitable for synthesis of dual bandpass filters as well as direct synthesis for single bandpass filters.},
 author = {Evaristo Musonda and Ian Hunter},
 booktitle = {2015 {IEEE} {MTT}-S International Microwave Symposium},
 doi = {10.1109/mwsym.2015.7166710},
 month = {may},
 publisher = {{IEEE}},
 title = {Synthesis of general Chebyshev characteristic function for dual (single) bandpass filters},
 url = {https://doi.org/10.1109%2Fmwsym.2015.7166710},
 year = {2015}
}

@article{Pelliccia_2013,
 abstract = {Ultra-compact band-pass filters with Nth-order pseudoelliptic response are presented allowing dramatic volume and mass savings (up to about 90%) compared to conventional filters at the price of a modest Q-factor reduction. TM dual-mode cavities loaded with high-permittivity (εr > 30) ceramic cylinders are employed, while non-resonating modes are used to create N transmission zeros symmetrically or non-symmetrically located in the pass-band. A single cavity behaves as a generalized doublet generating two transmission poles and two transmission zeros and is used as a basic building block for N-pole and N-zero filters with symmetric or asymmetric response. To prove the effectiveness of the approach, a very compact fourth-order filter with 4.35 GHz center frequency, four transmission zeros has been designed, fabricated, and tested showing an unloaded Q close to 2000. It is finally shown that the filter response can be shifted in frequency by simply replacing the dielectric material with another having different εr, so that the same metal enclosure can be used to realize various filters operating at different frequency bands.},
 author = {Luca Pelliccia and Fabrizio Cacciamani and Cristiano Tomassoni and Roberto Sorrentino},
 doi = {10.1017/s1759078713001001},
 journal = {International Journal of Microwave and Wireless Technologies},
 month = {dec},
 number = {2},
 pages = {151--159},
 publisher = {Cambridge University Press ({CUP})},
 title = {Ultra-compact high-performance filters based on {TM} dual-mode dielectric-loaded cavities},
 url = {https://doi.org/10.1017%2Fs1759078713001001},
 volume = {6},
 year = {2013}
}

@article{Rhodes_1980,
 abstract = {A synthesis method is presented for the class of low-pass prototype filters having an equiripple passband response with a single transmission zero at infinity and the remainder at a finite real frequency. To synthesize the network, the even mode or the odd mode is obtained directly using the alternating pole technique and little accuracy is lost for networks up to degree 19. Tables of element values for commonly used specifications are included. Finally, the practical advantages of this prototype when used to design certain classes of filters are discussed.},
 author = {J. D. Rhodes and S. A. Alseyab},
 doi = {10.1002/cta.4490080205},
 journal = {International Journal of Circuit Theory and Applications},
 month = {apr},
 number = {2},
 pages = {113--125},
 publisher = {Wiley},
 title = {The generalized chebyshev low-pass prototype filter},
 url = {https://doi.org/10.1002%2Fcta.4490080205},
 volume = {8},
 year = {1980}
}

