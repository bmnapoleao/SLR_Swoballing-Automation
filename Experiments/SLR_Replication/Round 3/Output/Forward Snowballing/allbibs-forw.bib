 title = {{ORGANIZATIONAL} {LEARNING} {APPLIED} {TO} {SOFTWARE} {ENGINEERING}: A {SYSTEMATIC} {REVIEW}},
 url = {https://doi.org/10.1142%2Fs0218194013500356},
 volume = {23},
 year = {2013}
