 title = {Value-Oriented Requirements Prioritization in a Small Development Organization},
 url = {https://doi.org/10.1109%2Fms.2007.30},
 volume = {24},
 year = {2007}
}

@incollection{Berente_2009,
 abstract = {The High Impact Design Requirements Project involves an international, cross-disciplinary group of researchers and practitioners
focused on setting the agenda for the design requirements discourse. The group’s initial workshop identified five key themes
that should receive more attention in design requirements and practice: (1) fundamental concepts of design, (2) evolution
and the fluidity of design, (3) quality and value-based requirements, (4) requirements intertwining, and (5) adapting requirements
practices in different domains. This chapter presents an introduction to the project, the workshop, and these five themes.},
 author = {Nicholas Berente and Sean Hansen and Kalle Lyytinen},
