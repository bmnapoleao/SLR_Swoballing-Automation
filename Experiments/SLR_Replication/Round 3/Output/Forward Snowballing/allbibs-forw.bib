 title = {A {SURVEY} {OF} {EXISTING} {REQUIREMENTS} {ENGINEERING} {TECHNOLOGIES} {AND} {THEIR} {COVERAGE}},
 url = {https://doi.org/10.1142%2Fs0218194007003513},
 volume = {17},
